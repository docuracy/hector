{
  "@context": {
    "la": "http://linked.art/ns/v1/",
    "skos": "http://www.w3.org/2004/02/skos/core#",
    "schema": "http://schema.org/",
    "lexvo": "http://lexvo.org/id/term/",
    "aat": "http://vocab.getty.edu/aat/",
    "wd": "http://www.wikidata.org/entity/",
    "qudt": "http://qudt.org/schema/qudt#",
    "rdfs": "http://www.w3.org/2000/01/rdf-schema#",
    "xsd": "http://www.w3.org/2001/XMLSchema#",
    "hector": "https://w3id.org/hector#",

    "hector:role": {
      "@id": "https://w3id.org/hector#role",
      "@type": "@id",
      "rdfs:label": "Role",
      "rdfs:comment": "The linguistic or usage role of a given name or term variant."
    },
    "hector:roleAttestedVariant": {
      "@id": "https://w3id.org/hector#attested_variant",
      "rdfs:label": "Attested Variant",
      "rdfs:comment": "A variant form of the term attested in historical sources."
    },
    "hector:roleModernLemma": {
      "@id": "https://w3id.org/hector#modern_lemma",
      "rdfs:label": "Modern Lemma",
      "rdfs:comment": "The modern standardized or lemma form of a term."
    },
    "hector:roleArchaicForm": {
      "@id": "https://w3id.org/hector#archaic_form",
      "rdfs:label": "Archaic Form",
      "rdfs:comment": "An archaic or obsolete form of the term."
    },

    "hector:source": {
      "@id": "https://w3id.org/hector#source",
      "rdfs:label": "Source",
      "rdfs:comment": "A bibliographic or textual source documenting the name, term, or taxation."
    },
    "hector:sourceText": {
      "@id": "https://w3id.org/hector#sourceText",
      "rdfs:label": "Source Text",
      "rdfs:comment": "Textual citation or excerpt from a source related to the data."
    },
    "hector:phoneticKey": {
      "@id": "https://w3id.org/hector#phoneticKey",
      "@container": "@list",
      "@type": "xsd:string",
      "rdfs:label": "Phonetic Key",
      "rdfs:comment": "A phonetic transcription or simplified phonetic form used for matching variant spellings of a commodity or term."
    },
    "hector:taxation": {
      "@id": "https://w3id.org/hector#taxation",
      "@type": "@id",
      "rdfs:label": "Taxation",
      "rdfs:comment": "A taxation charge or rate associated with the commodity, typically linked to a schema.org TaxChargeSpecification."
    },
    "hector:conversionToGram": {
      "@id": "https://w3id.org/hector#conversionToGram",
      "@type": "xsd:decimal",
      "rdfs:label": "Conversion to Gram",
      "rdfs:comment": "Conversion factor from the historical unit to grams."
    },
    "hector:historicalTerm": {
      "@id": "https://w3id.org/hector#historicalTerm",
      "rdfs:label": "Historical Term",
      "rdfs:comment": "A historical term or name associated with a unit, commodity, or concept."
    },
    "hector:unit": {
      "@id": "https://w3id.org/hector#unit",
      "@type": "@id",
      "rdfs:label": "Unit",
      "rdfs:comment": "Reference to a unit of measure, typically a QUDT unit."
    },
    "hector:validFrom": {
      "@id": "https://w3id.org/hector#validFrom",
      "@type": "xsd:gYear",
      "rdfs:label": "Valid From",
      "rdfs:comment": "Year from which the given data, name, or taxation is valid."
    },
    "hector:validThrough": {
      "@id": "https://w3id.org/hector#validThrough",
      "@type": "xsd:gYear",
      "rdfs:label": "Valid Through",
      "rdfs:comment": "Year until which the given data, name, or taxation is valid."
    },
    "hector:originPlace": {
      "@id": "https://w3id.org/hector#originPlace",
      "@type": "@id",
      "rdfs:label": "Origin Place",
      "rdfs:comment": "The place of origin or provenance of a commodity or term."
    },

    "rdfs:label": "http://www.w3.org/2000/01/rdf-schema#label",
    "rdfs:comment": "http://www.w3.org/2000/01/rdf-schema#comment"
  }
}
